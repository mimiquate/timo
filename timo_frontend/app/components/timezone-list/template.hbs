<div class="timezone-list">
  {{#each @timezones as |timezone|}}
    <TimezoneList::Timezone
      @timezone={{timezone}}
      @scheduleEvent={{@scheduleEvent}}
      @onHover={{this.onHover}}
      @mouseLeave={{this.mouseLeave}}
      @hover={{this.hover}}
      @scrollAll={{this.scrollAll}}
    />
  {{/each}}
</div>
